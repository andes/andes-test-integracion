  "devDependencies": {
    "@angular/cli": "^8.3.8",
    "@elastic/elasticsearch": "^6.8.6",
    "cypress": "^3.3.1",
    "cypress-plugin-retries": "^1.5.2",
    "mocha": "^4.1.0",
    "mocha-junit-reporter": "^1.23.2",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^4.0.0",
    "require-dir": "^1.2.0"
  },
  "dependencies": {
    "@octokit/auth-app": "^2.4.3",
    "@octokit/rest": "^17.1.0",
    "cross-env": "^6.0.3",
    "faker": "^4.1.0",
    "mongodb": "^3.3.4",
    "request": "^2.88.0"
  }
