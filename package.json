  },
  "homepage": "https://github.com/andes/andes-test-integracion#readme",
  "devDependencies": {
    "@angular/cli": "^8.3.8",
    "cypress": "^3.3.1"
  },
  "dependencies": {
    "cross-env": "^6.0.3",
    "faker": "^4.1.0",
    "mongodb": "^3.3.4",
    "request": "^2.88.0"
  }
}
